<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".activities.EndGameActivity"
    android:background="@color/light_blue">

    <LinearLayout
        android:id="@+id/achievements_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerInParent="true"
        android:layout_marginLeft="@dimen/result_list_margin"
        android:layout_marginRight="@dimen/result_list_margin" >

        <!--<LinearLayout-->
            <!--android:layout_width="@dimen/item_width"-->
            <!--android:layout_height="@dimen/item_height"-->
            <!--android:layout_marginTop="@dimen/items_top_margin"-->
            <!--android:clipChildren="false"-->
            <!--android:clipToPadding="false"-->
            <!--android:orientation="horizontal" >-->

            <!--<TextView-->
                <!--android:layout_width="@dimen/item_height"-->
                <!--android:layout_height="@dimen/item_height"-->
                <!--android:textSize="@dimen/achievements_numbers_size"-->
                <!--android:textColor="@color/black"-->
                <!--android:textStyle="bold"-->
                <!--android:background="@drawable/circle_yellow"-->
                <!--android:gravity="center"-->
                <!--android:layout_marginLeft="5dp" />-->

        <!--</LinearLayout>-->

        <LinearLayout
            android:layout_width="@dimen/item_width"
            android:layout_height="@dimen/item_height"
            android:layout_marginTop="@dimen/items_top_margin"
            android:orientation="horizontal"
            android:layout_weight="2"
            >

            <com.flycode.customtypefacewidget.CTTextView
                android:id="@+id/game_type"
                android:layout_width="@dimen/text_width"
                android:layout_height="@dimen/item_height"
                android:layout_weight="1"
                android:textSize="@dimen/button_text_size"
                android:textColor="@color/black"
                app:typeface="OpenSans-Regular.ttf"
                android:paddingLeft="@dimen/text_padding"
                android:paddingRight="@dimen/text_padding"
                android:background="@drawable/rounded_button_white"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:gravity="center" />


            <com.flycode.customtypefacewidget.CTTextView
                android:id="@+id/game_difficulty"
                android:layout_width="@dimen/text_width"
                android:layout_height="@dimen/item_height"
                android:layout_marginLeft="5dp"
                android:layout_weight="1"
                android:textSize="@dimen/button_text_size"
                android:textColor="@color/black"
                app:typeface="OpenSans-Regular.ttf"
                android:paddingLeft="@dimen/text_padding"
                android:paddingRight="@dimen/text_padding"
                android:background="@drawable/rounded_button_white"
                android:gravity="center"
                />


        </LinearLayout>

        <!--<LinearLayout-->
            <!--android:layout_width="@dimen/item_width"-->
            <!--android:layout_height="@dimen/item_height"-->
            <!--android:layout_marginTop="@dimen/items_top_margin"-->
            <!--android:orientation="horizontal"-->
            <!--&gt;-->

            <!--<com.flycode.customtypefacewidget.CTTextView-->
                <!--android:layout_width="@dimen/text_width"-->
                <!--android:layout_height="@dimen/item_height"-->
                <!--android:text="@string/completed_quizzes"-->
                <!--android:textSize="@dimen/button_text_size"-->
                <!--android:textColor="@color/black"-->
                <!--app:typeface="OpenSans-Regular.ttf"-->
                <!--android:paddingLeft="@dimen/text_padding"-->
                <!--android:paddingRight="@dimen/text_padding"-->
                <!--android:background="@drawable/rounded_button_white"-->
                <!--android:gravity="center"-->
                <!--/>-->

            <!--<TextView-->
                <!--android:id="@+id/completed_quizzes_count"-->
                <!--android:layout_width="@dimen/item_height"-->
                <!--android:layout_height="@dimen/item_height"-->
                <!--android:textSize="@dimen/achievements_numbers_size"-->
                <!--android:textColor="@color/black"-->
                <!--android:textStyle="bold"-->
                <!--android:background="@drawable/circle_yellow"-->
                <!--android:gravity="center"-->
                <!--android:layout_marginLeft="5dp"-->
                <!--/>-->

        <!--</LinearLayout>-->

        <LinearLayout
            android:layout_width="@dimen/item_width"
            android:layout_height="@dimen/item_height"
            android:layout_marginTop="@dimen/items_top_margin"
            android:orientation="horizontal"
            >

            <com.flycode.customtypefacewidget.CTTextView
                android:layout_width="@dimen/text_width"
                android:layout_height="@dimen/item_height"
                android:text="@string/right_answers"
                android:textSize="@dimen/button_text_size"
                android:textColor="@color/black"
                app:typeface="OpenSans-Regular.ttf"
                android:paddingLeft="@dimen/text_padding"
                android:paddingRight="@dimen/text_padding"
                android:background="@drawable/rounded_button_white"
                android:gravity="center"
                />

            <TextView
                android:id="@+id/right_answers_count"
                android:layout_width="@dimen/item_height"
                android:layout_height="@dimen/item_height"
                android:textSize="@dimen/achievements_numbers_size"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:background="@drawable/circle_yellow"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="@dimen/item_width"
            android:layout_height="@dimen/item_height"
            android:layout_marginTop="@dimen/items_top_margin"
            android:orientation="horizontal" >

            <com.flycode.customtypefacewidget.CTTextView
                android:layout_width="@dimen/text_width"
                android:layout_height="@dimen/item_height"
                android:text="@string/wrong_answers"
                android:textSize="@dimen/button_text_size"
                android:textColor="@color/black"
                app:typeface="OpenSans-Regular.ttf"
                android:paddingLeft="@dimen/text_padding"
                android:paddingRight="@dimen/text_padding"
                android:background="@drawable/rounded_button_white"
                android:gravity="center"
                />

            <TextView
                android:id="@+id/wrong_answers_count"
                android:layout_width="@dimen/item_height"
                android:layout_height="@dimen/item_height"
                android:textSize="@dimen/achievements_numbers_size"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:background="@drawable/circle_yellow"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                />

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>















